    "version": "0.2.0",
    "configurations": [
        {
            "name": "Examples: Server",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/server/server.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "cwd": "${workspaceFolder}/examples/server"
        },
        {
            "name": "Examples: Server Proto Generator",
            "type": "debugpy",
            "request": "launch",
            "module": "gr8gen",
            "justMyCode": false,
            "args": [
                "-s",
                "examples.server.service.main.ExampleService",
                "-o",
                "examples/server/proto"
            ]
        }
    ]
}